{
  "name": "tailwind-java-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "test": "ts-node src/index.ts",
    "start": "npm run clean && ts-node src/index.ts && npm run build:tailwind",
    "build": "tsc",
    "clean": "rm -rf output",
    "build:tailwind": "NODE_ENV=production postcss src/tailwind.css -o output/purge/index.css",
    "watch:tailwind": "postcss src/tailwind.css -o output/full/index.css -w"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.7.4",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.41",
    "typescript": "^3.5.3",
    "tailwindcss": "^1.1.2",
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "cssnano": "^4.1.10"
  }
}
