{
   "name": "tailwind-java-generator",
   "version": "1.0.10",
   "description": "A library to export a purges stylesheet of tailwind css",
   "main": "index.ts",
   "scripts": {
      "test": "ts-node src/test.ts",
      "start": "node dist/src/index.js & npm run build:tailwind",
      "start:ts": "ts-node src/index.ts && npm run build:tailwind",
      "build": "tsc",
      "clean": "rm -rf output",
      "build:tailwind": "postcss ./src/tailwind.css -o ./../../target/fdox-1.0.0.-SNAPSHOT/frontend/styles/index.css",
      "watch:tailwind": "postcss src/tailwind.css -o output/full/index.css -w",
      "prepublish": "tsc"
   },
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@types/node": "^12.7.4",
      "@fullhuman/postcss-purgecss": "^1.2.0",
      "ts-node": "^8.3.0",
      "ts-node-dev": "^1.0.0-pre.41",
      "typescript": "^3.5.3",
      "tailwindcss": "^1.1.2",
      "postcss": "^7.0.18",
      "postcss-cli": "^6.1.3",
      "postcss-import": "^12.0.1",
      "cssnano": "^4.1.10"
   },
   "devDependencies": {}
}
